  "repositories": [
  ],
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-openssl": "*",
    "brace/core": "*",
    "brace/mod-session": "*",
    "brace/mod-body": "*",
    "brace/mod-router": "*",
    "brace/mod-request-laminas": "*"
  },
  "require-dev": {
    "phore/tester": "*"
  },
  "suggest": {
  },
  "autoload": {
